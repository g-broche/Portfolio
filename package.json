{
  "name": "quick-parcel-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "cpy \"src/assets/pictures/**/*\" \"dist/src/assets/pictures\" && parcel index.html",
    "start-multiple-entries": "parcel index.html centered.html what.html ",
    "build": "cpy \"src/assets/pictures/**/*\" \"dist/src/assets/pictures\" && parcel build --public-url . index.html",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "assets": "^3.0.1",
    "parcel-plugin-asset-copier": "^1.1.0",
    "parcel-plugin-html-root-syntax": "0.0.3",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "cpy-cli": "^5.0.0",
    "gh-pages": "^3.0.0",
    "parcel-bundler": "^1.5.1",
    "sass": "^1.26.11",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [],
  "assetsPath": "assets"
}